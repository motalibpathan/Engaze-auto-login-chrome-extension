{
  "manifest_version": 3,
  "name": "Engaze Auto Login Extension",
  "description": "This extension will automatically login to Engaze UI for development purposes only.",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:3001/*"],
      "js": ["contentScript.js"]
    }
  ]
}
